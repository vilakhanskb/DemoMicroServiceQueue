{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "KafkaConfiguration": {
    "BootstrapServers": "127.0.0.1:9092",
    "DeadlineInSecond": 30,
    "MessageMaxBytes": 104857600,
    "QueueRequestService": "qrequestservice",
    "QueueMonUpdated": "qmonupdated",
    "QueueRequestSDK": "qrequestsdk",
    "QueueMonCreated": "qmoncreated"
  }
}
